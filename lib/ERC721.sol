contract ERC721ChildContract is ERC721A {
    using SafeMath for uint256;

    string public baseURI;
    uint public tokenId;
    uint public feeRate;
    uint public royaltyFeeRate = 2000;
    uint public tokenPrice;
    uint public hardcap;
    address public minter;
    address payable public feeTo;
    bool public isRoyaltyEnable;
    bool private _isTokenTransferEnable;
    address payable public  Owner;

    struct Details{
        uint TokenPrice;
        bool isForSale;
        bool isForAuction;
        string URI;
        uint bidStartTime;
        uint bidEndTime;
    }

    struct userBid
    {
      uint amount;
      address userAddress;
    }
    
    mapping(uint => userBid[]) public bidDetails ;
    
    mapping(uint => Details) public TokenDetails;

    constructor(address _deployer,  address _minterAddress, string memory _tokenName, string memory _tokenSymbol, uint _feeRate, bool _isTransferEnable,uint _hardcap) ERC721A(_tokenName, _tokenSymbol)  {
        minter = _minterAddress;
        Owner = payable(_minterAddress);
        feeTo = payable(_deployer);
        _setTokenTransferAction(_isTransferEnable);
        feeRate = _feeRate;
        hardcap=_hardcap;
    }

    modifier isOwner(address _owner)
    {
        require(Owner ==  _owner ,"Wallet Address is not an Owner");
        _;
    }

    function isTokenTransferEnable() public view  returns(bool){
        return _isTokenTransferEnable;
    }

    function _setTokenTransferAction(bool _enable)  internal {
        _isTokenTransferEnable = _enable;
    }

    function setRoyaltyFeeRate(uint _royaltyFeeRate) public isOwner(msg.sender){
        royaltyFeeRate = _royaltyFeeRate;
    }

    function setRoyaltyEnable(bool value) public isOwner(msg.sender) {
        isRoyaltyEnable = value;
    }

    function setTokenPrice(uint _price) public isOwner(msg.sender){
        tokenPrice = _price;
    }

    function setHardcap(uint _hardcap) public isOwner(msg.sender){
        hardcap = _hardcap;
    }

    // bulk mint
    function bulkMintNFT(address[] calldata _to, string[] calldata _uris) isOwner(msg.sender) external payable {
        require(_to.length == _uris.length, "ERC721ChildContract: Address[] length must be equal to amount[] length");
        require(msg.value == _to.length.mul(tokenPrice), "ERC721ChildContract: Invalid amount");
        if(hardcap != 0){
            require(tokenId.add(_to.length) < hardcap, "ERC721ChileContract: Max tokens minted");
        }
        uint fee = (msg.value).mul(feeRate).div(1e4);
        (bool sent, ) = feeTo.call{value: fee}("");
        require(sent, "Failed to send Ether");
        for(uint i=0; i<_to.length; i++) {
            _safeMint(_to[i], 1);
            _setTokenURI(tokenId, _uris[i]);
            tokenId++;
        }        
    }

    function singleMintNFT(address  _to, string memory _uri,uint256 _tokenPrice) isOwner(msg.sender) public payable {
        if(hardcap != 0){
            require(tokenId < hardcap, "ERC721ChileContract: Max tokens minted");
        }
        uint fee = (msg.value).mul(feeRate).div(1e4);
        (bool sent, ) = feeTo.call{value: fee}("");
        require(sent, "Failed to send Ether");         
        _safeMint(_to, 1);
        _setTokenURI(tokenId, _uri);
        TokenDetails[tokenId].TokenPrice=_tokenPrice;
        TokenDetails[tokenId].URI=_uri;
        TokenDetails[tokenId].isForSale = false;
        TokenDetails[tokenId].isForAuction =false;
        setRoyaltyEnable(true);
        tokenId++;                
    }



    // set token URI in Bulk
    function setTokenURIInBulk(uint[] memory _tokenId, string[] memory _uri) isOwner(msg.sender) public {
        require(_tokenId.length == _uri.length, "ERC721ChildContract: tokenId and URI length does not match");
        for(uint i=0; i<_tokenId.length; i++) {
            _setTokenURI(_tokenId[i], _uri[i]);
        }
    }

    function setTokenURI(uint _tokenId, string memory _uri) isOwner(msg.sender) public {
        bytes memory test = bytes(_uri);
        require(_tokenId >= 0 && test.length > 0, "ERC721ChildContract: tokenId and URI length does not match");       
        _setTokenURI(_tokenId, _uri);   
        TokenDetails[tokenId].URI=_uri;
    }

    function setTokenTransferAction(bool value) public {
        _setTokenTransferAction(value);
    }

     /**
     * @dev Transfers `tokenId` from `from` to `to`.
     *
     * Requirements:
     *
     * - `to` cannot be the zero address.
     * - `tokenId` token must be owned by `from`.
     *
     * Emits a {Transfer} event.
     */

    function _transfer(
        address from,
        address to,
        uint256 _tokenId
    ) internal  override{
        require(_isTokenTransferEnable, "ERC721: Token transfer is not allowed");
        super._transfer(from, to, _tokenId);
    }

    function claimTo(address userWallet,uint256 quantity) public payable
    {
        _safeMint(msg.sender, quantity); 
        // transferFrom(msg.sender, userWallet, tokenId);
        emit Transfer(msg.sender, userWallet,tokenId);
        tokenId++;
    }

    function price() public view returns (uint256){
        return tokenPrice;
    }

    function getClaimIneligibilityReason(address userWallet, uint256 quantity) public view returns (string memory){}
    
    function unclaimedSupply() public view returns (uint256){
        return hardcap.sub(tokenId);
    }  

    function withdrawFund(address destination) public  returns(bool){
        uint balance = address(this).balance;
        (bool success, ) = destination.call{value:balance}("");
        return success;
    }

    //-------------------------MarketPlaceFunc------------------------------------//  

    function Bid(uint _tokenID)public payable returns(bool){
     require(TokenDetails[_tokenID].bidStartTime < block.timestamp,"Sorry auction isn't started yet ");
     require(TokenDetails[_tokenID].bidEndTime > block.timestamp,"Sorry auction is ended ");
     require(TokenDetails[_tokenID].isForSale == true ,"Sorry this nft isn't for sale");
     require(TokenDetails[_tokenID].isForAuction == true,"Sorry this nft isn't for auction");
     require(msg.value <= address(msg.sender).balance,"Not enough balance");
     bidDetails[_tokenID].push(userBid({amount:msg.value,userAddress:msg.sender}));
     (bool success, ) = address(this).call{value: msg.value}("");
     return success;
    }   

    function getUserBidCount(uint _tokenID) public view returns(uint){
        return bidDetails[_tokenID].length;
    }

    function getRoyaltyFees(uint _tokenId, uint _index) internal view returns(uint) {
        return (bidDetails[_tokenId][_index].amount).mul(royaltyFeeRate).div(1e4);
    }

    function refundBid(uint _tokenID) public payable returns(bool _success)
    {
        bool _Success;
        for(uint i=0 ;i < bidDetails[_tokenID].length;i++)
        {
            if(bidDetails[_tokenID][i].userAddress == msg.sender)
            {
                require(bidDetails[_tokenID][i].amount > 1000000000,"No Bid Data Found");                
                (bool success, ) = bidDetails[_tokenID][i].userAddress.call{value:(bidDetails[_tokenID][i].amount - getRoyaltyFees(_tokenID, i))}("");
                _Success = success;               
                bidDetails[_tokenID][i].amount = 0 ;                              
            }           
        }
        return _Success;
    }

    function listTokenForSale(uint _tokenID,bool _actionForAuction ,uint _bidStartingTime,uint _bidEndingtime) public  isOwner(msg.sender){
        require(_tokenID < tokenId,"Invalid token Id");
        TokenDetails[_tokenID].isForSale = true;
        TokenDetails[_tokenID].isForAuction = _actionForAuction;
        if(_actionForAuction)
        {
         TokenDetails[_tokenID].bidStartTime = _bidStartingTime;
         TokenDetails[_tokenID].bidEndTime = _bidEndingtime;
        }                 
    }

    function updateAuctionTime(uint _tokenID,uint _bidStartingTime,uint _bidEndingtime) public isOwner(msg.sender){
        require( TokenDetails[_tokenID].isForAuction,"Token isn't listed for an auction");
        TokenDetails[_tokenID].bidStartTime = _bidStartingTime;
        TokenDetails[_tokenID].bidEndTime = _bidEndingtime;        
    }

    function unlistTokenForSale(uint _tokenID)public isOwner(msg.sender){
        TokenDetails[_tokenID].isForSale = false;
        TokenDetails[_tokenID].isForAuction = false;  
          if(bidDetails[_tokenID].length > 0)
        {
           delete bidDetails[_tokenID];
        }
    }

    function cancelAuction(uint _tokenID)public payable isOwner(msg.sender) returns(bool _success){
        bool _Success;
        require(TokenDetails[_tokenID].isForAuction,"Auction is already ended");
        TokenDetails[_tokenID].isForAuction = false;
        TokenDetails[_tokenID].bidStartTime = 0;
        TokenDetails[_tokenID].bidEndTime = 0;  
        if(bidDetails[_tokenID].length > 0)
        {
            for(uint i = 0; i < bidDetails[_tokenID].length; i++)
            {
                (bool success, ) = bidDetails[_tokenID][i].userAddress.call{value:bidDetails[_tokenID][i].amount}("");
               _Success = success;
            }
           delete bidDetails[_tokenID];
            return _Success;
        }
    }

    function buyToken(uint _tokenID)public payable
    {
        require(TokenDetails[_tokenID].isForSale,"Sorry you cann't but this token");
        require(address(msg.sender).balance >= TokenDetails[_tokenID].TokenPrice , "Sorry you haven't enough balance");
        Owner.transfer(TokenDetails[_tokenID].TokenPrice);
        transferFrom(Owner,msg.sender,_tokenID);
        TokenDetails[_tokenID].isForSale = false;
        TokenDetails[_tokenID].isForAuction = false;
    }
    
    function bidWinner(uint _tokenID) view public returns(userBid memory)
    {
        require(TokenDetails[_tokenID].isForAuction,"Not listed for Auction");
        require(TokenDetails[_tokenID].isForAuction && TokenDetails[_tokenID].bidEndTime < block.timestamp, "Auction is running");

        userBid memory temp = userBid(0,0x0000000000000000000000000000000000000000);

        for (uint i = 0; i < bidDetails[_tokenID].length; i++) 
        {
            if (i + 1 < bidDetails[_tokenID].length ) 
            {
                if (bidDetails[_tokenID][i].amount > bidDetails[_tokenID][i + 1].amount) 
                {
                    if (temp.amount <= bidDetails[_tokenID][i].amount) 
                    {
                        temp = bidDetails[_tokenID][i];
                    }
                } 
                else if (bidDetails[_tokenID][i].amount < bidDetails[_tokenID][i + 1].amount) 
                {
                    if (temp.amount <= bidDetails[_tokenID][i + 1].amount)
                    {
                        temp = bidDetails[_tokenID][i];
                    }
                }
            } 
            else if (i == bidDetails[_tokenID].length - 1 && bidDetails[_tokenID][i].amount >= temp.amount) 
            {
                temp = bidDetails[_tokenID][i];
            }
        }      
        return temp;
    }

    function refundBidAmount (uint _tokenID) public payable returns(bool _success)
    {
      require(TokenDetails[_tokenID].bidEndTime < block.timestamp, "Auction is running");
      userBid memory winner = bidWinner(_tokenID);
      bool _Success;
       for(uint i = 0 ; i < bidDetails[_tokenID].length; i++ ) 
       {
            if(winner.userAddress != msg.sender && winner.userAddress != bidDetails[_tokenID][i].userAddress )
            {
              (bool success, ) = bidDetails[_tokenID][i].userAddress.call{value:(bidDetails[_tokenID][i].amount - getRoyaltyFees(_tokenID, i))}("");
               _Success = success;
            }
            if(winner.userAddress == msg.sender && winner.userAddress == bidDetails[_tokenID][i].userAddress )
            {
                if(winner.amount > bidDetails[_tokenID][i].amount)
                {
                 (bool success, )  =  bidDetails[_tokenID][i].userAddress.call{value:(bidDetails[_tokenID][i].amount)}("");                    
                  _Success = success;
                }
            }           
       }       
       return _Success ;
    }

    function claimNFT(uint _tokenID) external
    {
      require(TokenDetails[_tokenID].bidEndTime < block.timestamp,"Auction is running");
      userBid memory winner = bidWinner(_tokenID);
      transferFrom(Owner,winner.userAddress,_tokenID);     
    }
}


contract NFTFactory
{
    struct ChildContractDetails {  
        uint tokenID;      
        string contractType; // add a contract type as well i.e. ERC721 or ERC1155
        address owner;
        address childContractAddress;
        string name;
        string symbol;
        uint hardcap;
    }

    uint public feeRate = 200;    

    uint private tokenCount;

    address public Owner;

    mapping(address  => ChildContractDetails[]) private childDetails;

    event newChildContractCreated(string tokenName, string tokenSymbol, address contractAddress);

    event SetFeeRate(uint fee);

    modifier isOwner(address _owner)
    {
        require(Owner ==  _owner ,"Wallet Address is not an Owner");
        _;
    }

    constructor()
    {
        Owner =msg.sender;
    }


  
    function getContractsDetails(address _owner) public view returns(ChildContractDetails[] memory)
    {
        return childDetails[_owner];
    }

    function _addChild( uint _tokenId, string memory _contractType, address _contractAddress,address _deployerAddress, string memory _name, string memory _symbol ,uint _hardcap) internal {
        childDetails[_deployerAddress].push(ChildContractDetails({tokenID:_tokenId,contractType:_contractType,owner:_deployerAddress,childContractAddress:_contractAddress,name:_name,symbol:_symbol,hardcap:_hardcap}));
    }

    function createNewERC721NFTContract( address _minterAddress, string calldata _tokenName, string calldata _tokenSymbol, bool _isTransferEnable,uint _hardcap) external {
        ERC721ChildContract ownershipChild = new ERC721ChildContract(Owner, _minterAddress, _tokenName, _tokenSymbol, feeRate, _isTransferEnable ,_hardcap);
        _addChild(tokenCount, "ERC721 Token Standard",address(ownershipChild), _minterAddress, _tokenName, _tokenSymbol,_hardcap);
        emit newChildContractCreated(_tokenName, _tokenSymbol, address(ownershipChild));
        tokenCount++;
    }

    function setFeeRate(uint _feeRate) public isOwner(msg.sender){
        feeRate = _feeRate;
    }    

    function transferOwnership(address _owner) public isOwner(msg.sender)
    {
        Owner = _owner;
    }
    
    function withdrawFund() public isOwner(msg.sender) returns(bool){
        uint balance = address(this).balance;
        (bool success, ) = msg.sender.call{value:balance}("");
        return success;
    }

    function renounceOwnership() external isOwner(msg.sender){
        transferOwnership(0x0000000000000000000000000000000000000000);
    }
}
